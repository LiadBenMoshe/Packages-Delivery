<div class="col-lg-3 col-md-6 col-sm-6" id="crad-id">
    <div class="card card-stats">
      <div class="card-header card-header-<%= color %> card-header-icon">
        <div class="card-icon">
          <i class="material-icons"><%= icon %>y</i>
        </div>
        <p class="card-category"><%= title %></p>
        <h3 class="card-title" id="<%= districtId %>"><%= value %>
        </h3>
        <h5><%= unit %></h5>
      </div>
      <div class="card-footer">
        <div class="stats">
          <i class="material-icons text-danger"><%= fotterIcon %></i>
          <a href="javascript:;"><%= fotterText %></a>
        </div>
      </div>
    </div>
  </div>